{
  "name": "horizon-api",
  "version": "1.0.0",
  "private": true,
  "description": "Express REST API for Horizon",
  "main": "server.js",
  "engines": {
    "node": "v12.11.1",
    "npm": "6.11.3"
  },
  "scripts": {
    "build-css": "node-sass reports -wo reports",
    "watch": "nodemon ./server.js",
    "start": "npm run watch",
    "migrate": "node migrate.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "gtfs-realtime-bindings": "0.0.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.9.2",
    "mongoose-autopopulate": "^0.9.1",
    "nodemon": "^1.19.4",
    "pug": "^2.0.4",
    "puppeteer": "^1.20.0"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "node-sass": "^4.13.1",
    "supertest": "^6.2.2"
  }
}
